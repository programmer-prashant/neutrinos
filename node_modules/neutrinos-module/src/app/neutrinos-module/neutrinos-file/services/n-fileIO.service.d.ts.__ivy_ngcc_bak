import { HttpClient } from '@angular/common/http';
export declare class NFileIOService {
    private http;
    systemService: any;
    appProperties: any;
    constructor(http: HttpClient);
    private getFileInfo;
    private getFormData;
    getPicture(cameraOptions: any): Promise<unknown>;
    scanPicture(scanOptions: any): Promise<unknown>;
    getBarcode(barcodeOptions: any): Promise<unknown>;
    getVideo(videoOptions: any): Promise<unknown>;
    getTts(ttsOptions: any): Promise<unknown>;
    getShake(shakeOptions: any): Promise<unknown>;
    getOcr(ocrOptions: any): Promise<unknown>;
    getFingerprint(fingerprintOptions: any): Promise<unknown>;
    upload(options: any): Promise<any>;
    download(options: any): Promise<any>;
    saveFile(data: Blob, filename: string): Promise<any>;
    private saveToBrowser;
    private createDirectory;
    private checkFileExist;
    private setHeaders;
}
