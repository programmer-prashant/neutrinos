export declare class UtilService {
    private platformType;
    private environment;
    constructor();
    /**
     * Returns 'mobile' or 'browser' based on the platform
     */
    getPlatformType(): string;
    /**
     * Returns env from neutrinos seed app
     */
    private getEnvironment;
    /**
     * Returns 'mobile' or 'browser' based on the platform
     */
    private checkPlatformType;
    getBaseLoginPath(): string;
    getWebLoginUrl(redirectTo: any): string;
    getMobileLoginUrl(): string;
    getWebLogoutUrl(redirectTo: any): string;
    getMobileLogoutUrl(): string;
    getUserInfoUrl(): string;
    getSSDBasePath(): string;
    /**
     *  Returns a absoulte url for the urlPath
     * @param urlPath - path relative to the current host
     */
    private calcRedirectUrl;
    /**
     *
     * @param property - environment property key
     */
    getEnvValue(property: any): any;
}
