import { EventEmitter } from '@angular/core';
import { HttpClient } from '@angular/common/http';
import { NTokenService } from './n-token.service';
import { NPubSubService } from './n-pubSub.service';
import { NLocalStorageService } from './n-localStorage.service';
import { NNotificationService } from './n-notification.service';
import { Observable } from 'rxjs';
export declare class NLoginService {
    private http;
    private pubSubService;
    private notificationService;
    private nLocalStorageService;
    private nTokenService;
    loginUrl: any;
    appProperties: any;
    systemService: any;
    nSessionStorage: any;
    uuid: any;
    details: any;
    loginCompleted: EventEmitter<any>;
    constructor(http: HttpClient, pubSubService: NPubSubService, notificationService: NNotificationService, nLocalStorageService: NLocalStorageService, nTokenService: NTokenService);
    login(userName: any, password: any, isRemember?: any): Observable<Object>;
    isLoggedIn(): Promise<boolean>;
}
