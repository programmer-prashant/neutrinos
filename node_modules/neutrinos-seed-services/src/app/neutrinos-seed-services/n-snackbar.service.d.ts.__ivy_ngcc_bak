import { Observable } from 'rxjs';
import { NPubSubService } from './n-pubSub.service';
export declare class NSnackbarService {
    private nPubSubService;
    snackBarListner: any;
    private snackService;
    constructor(nPubSubService: NPubSubService);
    get(): Observable<any>;
    add(data: {
        msg: string;
        timeout?: number;
        background?: string;
        color?: string;
        customClass?: any;
        action?: {
            text: string;
            onClick?: Function;
            color?: string;
        };
        onAdd?: Function;
        onRemove?: Function;
    }): void;
    remove(id: string): void;
    clear(): void;
    openSnackBar(message: any, timeout?: any): void;
}
