import { Observable } from 'rxjs';
import { NLogoutService } from './n-logout.service';
import { NPubSubService } from './n-pubSub.service';
export declare class NHTTPLoaderService {
    private nLogoutService;
    private nPubSubService;
    pendingRequests: number;
    showLoading: boolean;
    private _isHTTPRequestInProgress;
    _isHTTPRequestInProgress$: Observable<boolean>;
    constructor(nLogoutService: NLogoutService, nPubSubService: NPubSubService);
    isHTTPRequestInProgress(bool: any): void;
    alertError(error: any): void;
}
